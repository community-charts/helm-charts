            app.kubernetes.io/name: n8n-mcp-webhook
            app.kubernetes.io/part-of: n8n
            app.kubernetes.io/version: 1.0.0
            helm.sh/chart: n8n-1.0.0
        spec:
          containers:
            - args:
                - webhook
              command:
                - n8n
              env:
                - name: N8N_HIRING_BANNER_ENABLED
                  value: "false"
                - name: NODE_ENV
                  value: production
                - name: N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS
